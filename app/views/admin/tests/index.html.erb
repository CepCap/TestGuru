<p><%= link_to t('.link_to_new'), new_admin_test_path %></p>
<table class="table">
  <tr>
    <th class="sort-by-title pointer">
      <%= Test.human_attribute_name(:title) %>
      <%= octicon "arrow-up", class: 'text-success hide' %>
      <%= octicon "arrow-down", class: 'text-success hide' %>
    </th>
    <th> <%= Test.human_attribute_name(:level) %> </th>
    <th> <%= t('.question_count') %> </th>
  </tr>
  <%= render @tests %>
</table>
